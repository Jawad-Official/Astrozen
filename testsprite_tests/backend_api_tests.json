{
  "tests": [
    {
      "id": "AUTH001",
      "title": "User Registration with Valid Data",
      "description": "Test the POST /api/v1/auth/register endpoint to ensure a new user can register successfully with valid email, password, first_name, and last_name, and that the response returns appropriate success status (201) and user data.",
      "steps": [
        {
          "action": "send_request",
          "method": "POST",
          "url": "http://localhost:8080/api/v1/auth/register",
          "headers": {
            "Content-Type": "application/json"
          },
          "body": {
            "email": "testuser@astrozen.com",
            "password": "SecurePassword123!",
            "first_name": "Test",
            "last_name": "User"
          }
        },
        {
          "action": "assert",
          "status_code": 201,
          "body_contains": ["email", "id", "first_name", "last_name"]
        }
      ]
    },
    {
      "id": "AUTH002",
      "title": "User Login with Correct Credentials",
      "description": "Test the POST /api/v1/auth/login endpoint to verify that a user can log in with valid username and password, and receives a valid JWT access token.",
      "steps": [
        {
          "action": "send_request",
          "method": "POST",
          "url": "http://localhost:8080/api/v1/auth/login",
          "headers": {
            "Content-Type": "application/x-www-form-urlencoded"
          },
          "body": "username=testuser@astrozen.com&password=SecurePassword123!"
        },
        {
          "action": "assert",
          "status_code": 200,
          "body_contains": ["access_token", "token_type"]
        }
      ],
      "depends_on": ["AUTH001"]
    },
    {
      "id": "AUTH003",
      "title": "Get Current User Info",
      "description": "Test the GET /api/v1/auth/me endpoint to verify that authenticated users can retrieve their current user information.",
      "steps": [
        {
          "action": "send_request",
          "method": "GET",
          "url": "http://localhost:8080/api/v1/auth/me",
          "headers": {
            "Authorization": "Bearer {{access_token}}"
          }
        },
        {
          "action": "assert",
          "status_code": 200,
          "body_contains": ["id", "email", "first_name", "last_name"]
        }
      ],
      "depends_on": ["AUTH002"]
    },
    {
      "id": "AUTH004",
      "title": "Login with Invalid Credentials",
      "description": "Test the POST /api/v1/auth/login endpoint with invalid password to verify proper error handling.",
      "steps": [
        {
          "action": "send_request",
          "method": "POST",
          "url": "http://localhost:8080/api/v1/auth/login",
          "headers": {
            "Content-Type": "application/x-www-form-urlencoded"
          },
          "body": "username=testuser@astrozen.com&password=WrongPassword!"
        },
        {
          "action": "assert",
          "status_code": 401
        }
      ]
    },
